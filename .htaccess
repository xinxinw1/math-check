RewriteEngine on

RewriteRule ^([^1/][^/]*)/prec-math-check\.(.*)$ $1/math-check.$2 [L]
RewriteRule ^1\.([^/]+)/math-check\.(.*)$ 1.$1/prec-math-check.$2 [L]

RewriteRule ^1\.x/(.*)$ 1.0/$1 [L,env=nocache:true]
RewriteRule ^2\.x/(.*)$ 2.1/$1 [L,env=nocache:true]
RewriteRule ^latest/(.*)$ 2.x/$1 [L,env=nocache:true]

Header set Cache-Control no-cache env=REDIRECT_nocache
